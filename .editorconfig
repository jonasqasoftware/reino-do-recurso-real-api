# Arquivo .editorconfig para o projeto "Reino do Recurso Real"

# Padrão para arquivos de texto
[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# Padrão para arquivos Markdown
[*.md]
trim_trailing_whitespace = false
